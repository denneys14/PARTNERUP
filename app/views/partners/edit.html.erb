<div class="container">
<p>
  <div>
  <h3>Edit your partners profil</h2>
  </p>
    <div>
    <%= simple_form_for(@partner) do |f| %>
      <%= f.input :name %>
      <%= f.input :gender %>
      <%= f.input :duty %>
      <%= f.input :description %>
      <%= f.input :age %>
    </div>
  </div>
  
  <%= f.submit class: "btn btn-ghost" %>
<% end %>

<%= link_to "Partner", partner_path, class: "btn btn-ghost" %>
</div>

