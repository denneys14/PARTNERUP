<div class="edit-container">
  <div class="col-12">
    <h3>Update your partners profile</h3>
    <%= simple_form_for(@partner) do |f| %>
    <%= f.input :name %>
    <%= f.input :duty %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :gender %>
    <%= f.input :age %>
    <%= f.input :photo, as: :file %>
    <p>or choose from web</p>
    <%= f.input :url_photo, as: :string %>
    <%= f.submit class: "btn btn-ghost" %>
    <% end %>
  <%#= link_to "Partner", partner_path, class: "btn btn-ghost" %>
 </div>
</div>

